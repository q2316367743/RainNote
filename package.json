{
    "id": "RainNote",
    "name": "{{LNG['note.meta.name']}}",
    "title": "{{LNG['note.meta.title']}}",
    "menu": "{{LNG['note.meta.menu']}}",
    "version": "1.0",
    "source": {
        "icon": "{{pluginHost}}static/image/icon.png",
        "screenshoot": [
            "{{pluginHost}}static/image/desc1.png",
            "{{pluginHost}}static/image/desc2.png",
            "{{pluginHost}}static/image/desc3.png"
        ]
    },
    "category": "tools,file",
